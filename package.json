  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack --mode production",
    "start:client": "webpack --mode development --watch",
    "start:server": "nodemon index.js --watch dist/js/main.server.js",
    "start": "rm -rf dist && NODE_ENV=development npm-run-all --print-label --parallel start:*",
    "spa": "NODE_ENV=development webpack serve --mode development --config webpack/configs/spa.config.ts",
    "test": "jest --coverage --coverage-reporters=lcov",
    "postinstall": "husky install",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "lint": "run-p lint:**",
    "lint:script": "eslint --fix --cache --quiet --ext .ts,.tsx ./",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\""
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{html,md,json}": [
      "prettier --write"
    ]
  },
